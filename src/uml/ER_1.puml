MetadataGroup <-- MetadataGroup.value

MetadataGroup "0,*"-l->"0,1" File
MetadataGroup "0,*"-r->"0,1" Dataverse
MetadataGroup "0,*"-->"0,1" Dataset
MetadataGroup "1,1"--"0,*" Metadata

Metadata <-- Metadata.id
