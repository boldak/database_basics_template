Metadata <-- Metadata.value

File <-- File.id
File <-- File.content

Dataset <-- Dataset.id
Dataset "1,1"--"0,*" File

Author "0,*"--|>"1,*" Acess
Admin "0,*"--|>"1,1" Acess
User <|-l- Acess
Acess <-- Acess.role
User <-- User.id
User <-- User.nickname
User <-- User.email
User <-- User.password
